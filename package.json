{
  "name": "taxi-tpv-tickets-api",
  "version": "0.0.1",
  "description": "An API for taxi tickets registration",
  "main": "index.js",
  "scripts": {
    "local": "cross-env SERVICE_ENV=local node index.js",
    "start": "node index.js",
    "test-ci": "cross-env SERVICE_ENV=ci mocha --reporter mocha-junit-reporter --timeout 5000 --exit",
    "test": "cross-env SERVICE_ENV=test mocha --reporter spec './test/**/*.test.js' --timeout 5000 --exit",
    "build": "echo 'No need for a build process'",
    "lint": "eslint .",
    "load": "DEBUG=* node db/loadData.js",
    "manifest": "node_modules/make-manifest/bin/make-manifest",
    "qa": "npm run lint && npm run test",
    "docker": "docker-compose --file docker/development/docker-compose.yml up -d --remove-orphans"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "cross-env": "^7.0.3",
    "eslint": "^6.7.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-mocha": "^6.2.2",
    "expect.js": "^0.3.1",
    "husky": "^3.1.0",
    "make-manifest": "^1.0.1",
    "mocha": "^7.2.0",
    "mocha-junit-reporter": "^1.23.1",
    "sinon": "^7.5.0",
    "supertest": "^4.0.2"
  },
  "dependencies": {
    "@google-cloud/vision": "^2.1.2",
    "body-parser": "^1.19.0",
    "boom": "^7.3.0",
    "bunyan": "^1.8.13",
    "chalk": "^3.0.0",
    "confabulous": "^2.1.0",
    "cors": "^2.8.5",
    "date-fns": "^2.13.0",
    "dayjs": "^1.11.3",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "error-handler-module": "^1.0.3",
    "express-fileupload": "^1.1.10",
    "express-jsdoc-swagger": "^1.7.0",
    "hogan.js": "^3.0.2",
    "jsonwebtoken": "^9.0.0",
    "make-manifest": "^1.0.1",
    "mongodb": "^3.5.7",
    "on-headers": "^1.0.2",
    "optional": "^0.1.4",
    "pdf-parse": "^1.1.1",
    "prepper": "^1.2.0",
    "ramda": "^0.27.0",
    "snyk": "^1.425.4",
    "systemic": "^3.3.7",
    "systemic-domain-runner": "^1.1.0",
    "systemic-express": "^1.1.1"
  },
  "snyk": true
}
